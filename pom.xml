        </dependency>
        <dependency>
            <groupId>org.mule.connectors</groupId>
            <artifactId>mule-objectstore-connector</artifactId>
            <classifier>mule-plugin</classifier>
        </dependency>
        <dependency>
            <groupId>3b8fd8b9-8002-4652-99bf-99d1c946b0ee</groupId>
            <artifactId>aduro-json-logger</artifactId>
            <version>${json-logger-version}</version>
            <classifier>mule-plugin</classifier>
        </dependency>
        <dependency>
            <groupId>3b8fd8b9-8002-4652-99bf-99d1c946b0ee</groupId>
            <artifactId>aduro-error-handling-plugin</artifactId>
            <version>${error-handling-plugin-version}</version>
            <classifier>mule-plugin</classifier>
        </dependency>
        <dependency>
            <groupId>org.mule.modules</groupId>
            <artifactId>mule-validation-module</artifactId>
            <version>2.0.6</version>
            <classifier>mule-plugin</classifier>
        </dependency>
		<dependency>
            <groupId>com.mulesoft.connectors</groupId>
            <artifactId>anypoint-mq-connector</artifactId>
            <version>4.0.12</version>
            <classifier>mule-plugin</classifier>
        </dependency>
		<dependency>
            <groupId>com.mulesoft.connectors</groupId>
            <artifactId>mule-salesforce-connector</artifactId>
            <version>11.1.0</version>
            <classifier>mule-plugin</classifier>
        </dependency>
        <!-- Aduro RAML Dependency -->
        <dependency>
            <groupId>3b8fd8b9-8002-4652-99bf-99d1c946b0ee</groupId>
            <artifactId>aduro-salesforce-fullload-prc-api</artifactId>
            <version>1.0.0</version>
            <classifier>raml</classifier>
            <type>zip</type>
        </dependency>
    </dependencies>

