            <id>Aduro-Exchange</id>
            <name>Aduro Exchange Repository</name>
            <url>https://maven.anypoint.mulesoft.com/api/v3/organizations/3b8fd8b9-8002-4652-99bf-99d1c946b0ee/maven</url>
